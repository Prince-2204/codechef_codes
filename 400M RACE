#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t ;
	cin >> t ;
	while(t--)
	{
	    // int x , y , z ;
	    // cin >> x >> y >> z ;
        int a[3];
	    // int r = 10000 ;
        // int index = -1 ;

        for (int i = 0; i < 3; i++)
        {
            cin >> a[i];
        }

        int x = *min_element(a , a + 3);

        if(x == a[0])
        {
            cout<<"ALICE"<<'\n';
        }
        else if(x == a[1])
        {
            cout<<"BOB"<<'\n';
        }else{
            cout<<"CHARLIE"<<'\n';
        }


        
        
        
	
	}
	return 0;
}

#include<bits/stdc++.h>

using ll = long long ;
using namespace std ;

int main()
{
    ll t ;
    cin >> t ;
    while (t--)
    {
        ll n , k ;
        cin >> n >> k ;
        ll arr[n] ;
        for (ll i = 0; i < n; i++)
        {
                cin >> arr[i] ;
        }

        for (ll i = 0; i < n; i++)
        {
            if((k > arr[i]) || (k == arr[i]))
            {
                cout<<'1';
                k = k - arr[i] ;
            }
            else
            {
                cout<<'0';
            }
            
        }
        cout<<'\n';
        
    }
    
}
